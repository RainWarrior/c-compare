void bubblesort(int zx[], int n){
    int i, j, k;
    for(i=0; i < n; ++i){
        for (j = i+1; j<n; ++j)
            if (zx[i] < zx[j])
                k = zx[i];
                zx[i] = zx[j];
                zx[j] = k;
    }
}

